	::= { cerent15216EdfaFfsFileTable  1}

Cerent15216EdfaFfsFileEntry ::= 
	SEQUENCE {
		cerent15216EdfaFfsFileName	     	DisplayString,
		cerent15216EdfaFfsFileType	     	INTEGER,
		cerent15216EdfaFfsFileStatus    	INTEGER,
		cerent15216EdfaFfsFileLength    	Integer32,
		cerent15216EdfaFfsFileAttributes  	Unsigned32
		 }

cerent15216EdfaFfsFileName OBJECT-TYPE
	SYNTAX  DisplayString (SIZE(1..20))
	MAX-ACCESS     not-accessible
	STATUS      current
	DESCRIPTION
		"The name of this file."
	::= { cerent15216EdfaFfsFileEntry 1 }

cerent15216EdfaFfsFileType OBJECT-TYPE
	SYNTAX INTEGER {
			directory(1), 
			sequenced(2),
			record(3),
			unknown(4)
			}
	MAX-ACCESS     read-only
	STATUS      current
