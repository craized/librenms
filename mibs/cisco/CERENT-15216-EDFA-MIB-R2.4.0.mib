cerent15216EdfaBootImageRevMajor	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaRevision
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"The major revision number of the image."
	::=  {  cerent15216EdfaBootImageEntry  5  }


cerent15216EdfaBootImageRevMinor	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaRevision
