-- community table

cerent15216EdfaCommunityTable	OBJECT-TYPE
	SYNTAX		SEQUENCE  OF
			    Cerent15216EdfaCommunityEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION	"The community database.
	    A multi-step set operation is used for row creation."
	::=  { cerent15216EdfaAgentCommObjects  2 }

cerent15216EdfaCommunityEntry	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaCommunityEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION	"The access privileges and MIB view
	for a particular community."
	INDEX		{  cerent15216EdfaCommIndex  }
	::=  { cerent15216EdfaCommunityTable 1 }

Cerent15216EdfaCommunityEntry  ::=  SEQUENCE {
	cerent15216EdfaCommIndex  Cerent15216EdfaIndex,
	cerent15216EdfaCommName  OCTET STRING,
	cerent15216EdfaCommViewIndex  Cerent15216EdfaIndex,
	cerent15216EdfaCommPrivileges  Integer32,
	cerent15216EdfaCommSrcIPAddr  IpAddress,
	cerent15216EdfaCommNetMask  IpAddress,
	cerent15216EdfaCommStatus  RowStatus
	}

cerent15216EdfaCommIndex	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaIndex
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"A community index uniquely identifying a
	                particular SNMPv1/v2c community."
	::=  {  cerent15216EdfaCommunityEntry  1  }


cerent15216EdfaCommName	OBJECT-TYPE
	SYNTAX		OCTET STRING  ( SIZE ( 0 .. 64  ) )
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION	"The community name, which is used in the
                        SNMP pdu's belonging to this community."
	DEFVAL			{ ''H }
	::=  {  cerent15216EdfaCommunityEntry  2  }


cerent15216EdfaCommViewIndex	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaIndex
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION	"The index value for the view table
			entries used by this community.  0 is a
			special case meaning unset.  An entry
			cannot be set to active until this value
			has been set."
	DEFVAL			{ 1 }
	::=  {  cerent15216EdfaCommunityEntry  3  }


cerent15216EdfaCommPrivileges	OBJECT-TYPE
	SYNTAX		Integer32  ( 0 .. 255  )
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION	"The access privileges which govern what
	                management operations a particular
	                community may perform.  These privileges
	                are specified as a sum of values, where
	                each value specifies a SNMP PDU type by
	                which the subject party may request a
	                permitted operation.  The value for a
	                particular PDU type is computed as 2
	                raised to the value of the ASN.1
	                context-specific tag for the appropriate
	                SNMP PDU type.  The values are:

	                Get         :   1
	                GetNext     :   2
	                Response    :   4
	                Set         :   8
	                SNMPv1-Trap :  16
	                GetBulk     :  32
	                Inform      :  64
	                SNMPv2-Trap : 128

			The null set is represented by the value
	                zero."
	DEFVAL			{ 35 }
	::=  {  cerent15216EdfaCommunityEntry  4  }


cerent15216EdfaCommSrcIPAddr	OBJECT-TYPE
	SYNTAX		IpAddress
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION	"The IP address from which network
	                management traffic for this community is
	                received.  The value 0.0.0.0 is a
	                special case meaning any IP address."
	DEFVAL			{ '00000000'H }
	::=  {  cerent15216EdfaCommunityEntry  5  }


cerent15216EdfaCommNetMask	OBJECT-TYPE
	SYNTAX		IpAddress
	MAX-ACCESS	read-create
