				   constCurrent ( 4 ) ,
				   constPower ( 5 ) ,
				   idle ( 6 ) }

Cerent15216EdfaBootSource ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"The list of boot source types."
	SYNTAX 		INTEGER  { tftp ( 1 ) ,
