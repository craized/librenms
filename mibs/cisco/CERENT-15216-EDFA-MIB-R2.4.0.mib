-- alarm objects

cerent15216EdfaAlarmLcrnt1	OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION	"Excessive pump current - pump 1.
	    Drive current greater than 95% of end of life value.
	    Current must drop to 90% of end of life value for 
	    alarm to clear."
	::=  {  cerent15216EdfaAlarmsMisc  1  }

cerent15216EdfaAlarmLcrnt2	OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION	"Excessive pump current - pump 2.
	    Drive current greater than 95% of end of life value. 
	    Current must drop to 90% of end of life value for 
	    alarm to clear."
	::=  {  cerent15216EdfaAlarmsMisc  2  }

cerent15216EdfaAlarmLtmp1	OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION	"Pump 1 laser temp. out of range.
	    Chip temperature deviating more than 10 degrees C 
	    from setpoint."
	::=  {  cerent15216EdfaAlarmsMisc  3  }

cerent15216EdfaAlarmLtmp2	OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION	"Pump 2 laser temp. out of range.
	    Chip temperature deviating more than 10 degrees C 
	    from setpoint."
	::=  {  cerent15216EdfaAlarmsMisc  4  }

cerent15216EdfaAlarmLpout	OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION	"Loss of output power.
	    EDFA output power is deviating more than the 
	    prescribed amount from the setpoint." 
	::=  {  cerent15216EdfaAlarmsMisc  5  }

cerent15216EdfaAlarmLpin	OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION	"Loss of input power.
	    EDFA input power is below the loss of signal 
	    threshold."
	::=  {  cerent15216EdfaAlarmsMisc  6  }

cerent15216EdfaAlarmGain	OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION	"Gain out of range.
	    This alarm is only valid when the ONS 15216 EDFA2 is in 
            GAINTEMP mode. It occurs when the value of  OVERALLGAINMEASURED 
            has deviated more than 1.25 dB from the manufacturer-defined set
            point’s allowable range. This condition has a 0.25 dB hysteresis.
            The GAIN alarm is also triggered if the value of INPWRDBM goes 
            outside the manufacturer-defined range by more than 0.7dB. This 
            condition has a 0.2 dB hysteresis.Gain has deviated more than 
            1.25dB from the setpoint, or input power is out of range for 
            setpoint."
	::=  {  cerent15216EdfaAlarmsMisc  7  }

cerent15216EdfaAlarmCtmp	OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION	"Case temperature out of the prescribed
	    range." 
	::=  {  cerent15216EdfaAlarmsMisc  8  }

cerent15216EdfaAlarmPowerBusA OBJECT-IDENTITY
	STATUS	current
	DESCRIPTION
		"The Power BUS A voltage is out of the prescribed range."
	::= { cerent15216EdfaAlarmsMisc 9 }

cerent15216EdfaAlarmPowerBusB OBJECT-IDENTITY
	STATUS	current
	DESCRIPTION
		"The Power BUS B voltage is out of the prescribed range."
	::= { cerent15216EdfaAlarmsMisc 10 }

cerent15216EdfaAlarmDbBackup OBJECT-IDENTITY
