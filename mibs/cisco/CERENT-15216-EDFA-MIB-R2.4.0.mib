	DESCRIPTION
		"The status of this file."
	::= { cerent15216EdfaFfsFileEntry 3 }
	
cerent15216EdfaFfsFileLength OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"The length of this file (-1 for 
		cerent15216EdfaFfsFileType = record)."
	::= { cerent15216EdfaFfsFileEntry 4 }
	
cerent15216EdfaFfsFileAttributes OBJECT-TYPE
	SYNTAX Unsigned32 
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"The attributes field for this file."
	::= { cerent15216EdfaFfsFileEntry 5 }

-- File System Operations

cerent15216EdfaFfsOpGroup  OBJECT-IDENTITY
	STATUS     current
	DESCRIPTION
		"The operations that can be performed on the
		of the Flash File System.

		An action may only be started when the value
		of the cerent15216EdfaFileOpStatus field is idle.
	
		To perform an action, the manager sets the 
		attributes as per the following:
		    Operation	                Fields
		    cerent15216EdfaFfsTftpPut  cerent15216EdfaFfsOpOperation
		                                cerent15216EdfaFfsOpRequestId
				                cerent15216EdfaFfsOpSrcName
				                cerent15216EdfaFfsOpDstName
				                cerent15216EdfaFfsOpTftpSrvAddr
		    cerent15216EdfaFileTftpGet	cerent15216EdfaFfsOpOperation
					        cerent15216EdfaFfsOpRequestId
					        cerent15216EdfaFfsOpSrcName
					        cerent15216EdfaFfsOpDstName
					        cerent15216EdfaFfsOpTftpSrvAddr
		    cerent15216EdfaFfsCopy	cerent15216EdfaFfsOpOperation
					        cerent15216EdfaFfsOpRequestId
					        cerent15216EdfaFfsOpSrcName
					        cerent15216EdfaFfsOpDstName
		    cerent15216EdfaFfsRename	cerent15216EdfaFfsOpOperation
					        cerent15216EdfaFfsOpRequestId
					        cerent15216EdfaFfsOpSrcName
					        cerent15216EdfaFfsOpDstName
		    cerent15216EdfaFfsDelete	cerent15216EdfaFfsOpOperation
					        cerent15216EdfaFfsOpRequestId
					        cerent15216EdfaFfsOpSrcName
		    cerent15216EdfaFfsCompress	cerent15216EdfaFfsOpOperation
					        cerent15216EdfaFfsOpRequestId
                    
		The manager then sets the cerent15216EdfaFfsOpAction field
		to perform. This will cause a cerent15216EdfaFfsOpStarted 
                trap to be generated.

		To abort the operation the manager sets the 
		cerent15216EdfaFfsOpAction field to abort.

		The cerent15216EdfaFfsOpFinished trap will be generated at 
		the completion (whether errored or not) of the 
		action."
	::= { cerent15216EdfaFfsActions 1 }

cerent15216EdfaFfsOpAction OBJECT-TYPE
	SYNTAX Cerent15216EdfaAction
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
		"Start or abort a file operation."
	DEFVAL      { idle }
	::= { cerent15216EdfaFfsOpGroup 1 }

cerent15216EdfaFfsOpOperation OBJECT-TYPE
	SYNTAX INTEGER {cerent15216EdfaFfsNoOperation(1), 
			cerent15216EdfaFfsTftpGet(2),
			cerent15216EdfaFfsTftpPut(3),
			cerent15216EdfaFfsTftpGetRecord(4),
			cerent15216EdfaFfsTftpPutRecord(5),
			cerent15216EdfaFfsCopy(6),
			cerent15216EdfaFfsRename(7),
			cerent15216EdfaFfsCompress(8),
			cerent15216EdfaFfsDelete(9)
		       }

	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
		"Indicates the current (or last) action being 
		performed on this flash file system.

		Note that cerent15216EdfaFfsTftpGet and 
                cerent15216EdfaFfsTftpPut transfer
		normal text or image files."    
	DEFVAL      { cerent15216EdfaFfsNoOperation }
	::= { cerent15216EdfaFfsOpGroup 2 }

cerent15216EdfaFfsOpStatus OBJECT-TYPE
	SYNTAX Cerent15216EdfaActionStatus
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"The status of the operation."
	DEFVAL      { idle }
	::= { cerent15216EdfaFfsOpGroup 3 }

cerent15216EdfaFfsOpStage OBJECT-TYPE
	SYNTAX INTEGER {
			idle(1), 
			flashDirOpen(2), 
			flashDirClose(3), 
			srcFileOpen(4), 
			dstFileOpen(5), 
			srcFileClose(6), 
			dstFileClose(7), 
			readData(8), 
			writeData(9), 
			rename(10), 
			delete(11), 
			findFile(12),
			compress(13),
			nextRecord(14)
			}
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Indicates the current (or last) stage of the 
		action being performed on this flash file system."
	DEFVAL      { idle }
	::= { cerent15216EdfaFfsOpGroup 4 }

cerent15216EdfaFfsOpCopyProgress OBJECT-TYPE
	SYNTAX INTEGER  (0..99999999) 
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"This provides an indication of the progress
		of the copy operation.

		For normal files this shows bytes copied."
	::= { cerent15216EdfaFfsOpGroup 5 }

cerent15216EdfaFfsOpResult OBJECT-TYPE
	SYNTAX INTEGER (0..999) 
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Indicates the result of the last operation.
		Success is indicated by a value of 0."
	::= { cerent15216EdfaFfsOpGroup 6 }

cerent15216EdfaFfsOpRequestId OBJECT-TYPE
	SYNTAX INTEGER (0..999) 
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
		"The request identifier for this operation.

		 This is provided to allow manangers to match
		 actions and results."
	::= { cerent15216EdfaFfsOpGroup 7 }

cerent15216EdfaFfsOpSrcName OBJECT-TYPE
	SYNTAX DisplayString (SIZE(0..127)) 
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
		"Source file name for the operation.

		Note that Flash File names are limited
		to 20 bytes, the longer name is provided
		support long path names for TFTP 
		operations from servers."
	::= { cerent15216EdfaFfsOpGroup 8 }

cerent15216EdfaFfsOpDstName OBJECT-TYPE
	SYNTAX DisplayString (SIZE(0..127)) 
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
		"Destination file name for the operation

		Note that Flash File names are limited
		to 20 bytes, the longer name is provided
		support long path names for TFTP 
		operations to servers."
	::= { cerent15216EdfaFfsOpGroup 9 }

cerent15216EdfaFfsOpTftpSrvAddr OBJECT-TYPE
	SYNTAX IpAddress
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
		"TFTP file server address."
	DEFVAL      { '00000000'H }
	::= { cerent15216EdfaFfsOpGroup 10 }

--
-- Notifications 
--

cerent15216EdfaOpFinished	NOTIFICATION-TYPE
	OBJECTS		{ cerent15216EdfaOpSaveConfig ,
			  cerent15216EdfaOpCutover ,
			  cerent15216EdfaOpLoad ,
			  cerent15216EdfaOpSrcFileName ,
			  cerent15216EdfaOpTftpSrvAddr ,
			  cerent15216EdfaOpReset ,
			  cerent15216EdfaOpResult ,
			  cerent15216EdfaOpRequestId,
                          cerent15216EdfaOpDbFileName,
                          cerent15216EdfaOpDbBackup,
                          cerent15216EdfaOpDbRestore  }
	STATUS		current
	DESCRIPTION	" Op Finished.
	    This trap indicates that an operation has 
	    completed."
	::=  {  cerent15216EdfaEventsV2  1  }

cerent15216EdfaAlarmActivated	NOTIFICATION-TYPE
	OBJECTS		{ cerent15216EdfaAlarmID ,
	                  cerent15216EdfaAlarmPriority ,
			  cerent15216EdfaAlarmDateAndTime  }
	STATUS		current
	DESCRIPTION	"Alarm Activated.
	    A notification indicating an alarm has changed to an 
	    active state."
	::=  {  cerent15216EdfaEventsV2  2  }

cerent15216EdfaAlarmCleared	NOTIFICATION-TYPE
	OBJECTS		{ cerent15216EdfaAlarmID ,
	                  cerent15216EdfaAlarmPriority ,
			  cerent15216EdfaAlarmDateAndTime  }
	STATUS		current
	DESCRIPTION	"Alarm Cleared.
	    A notification indicating an alarm has changed to a 
	    cleared state." 
	::=  {  cerent15216EdfaEventsV2  3  }

cerent15216EdfaFfsOpFinished NOTIFICATION-TYPE
	OBJECTS { cerent15216EdfaFfsOpOperation, 
                  cerent15216EdfaFfsOpStage,
                  cerent15216EdfaFfsOpCopyProgress,
		  cerent15216EdfaFfsOpResult, 
		  cerent15216EdfaFfsOpRequestId,
		  cerent15216EdfaFfsOpSrcName,
		  cerent15216EdfaFfsOpDstName,
	          cerent15216EdfaFfsOpTftpSrvAddr }
	 STATUS  current
	 DESCRIPTION
		"File System Op Finished.
		This trap indicates that the file
		operation has completed or aborted."
	 ::= { cerent15216EdfaEventsV2 4 }

cerent15216EdfaFfsOpStarted NOTIFICATION-TYPE
	OBJECTS { cerent15216EdfaFfsOpOperation, 
		  cerent15216EdfaFfsOpRequestId,
		  cerent15216EdfaFfsOpSrcName,
		  cerent15216EdfaFfsOpDstName,
	          cerent15216EdfaFfsOpTftpSrvAddr }
	 STATUS  current
	 DESCRIPTION
		"File System Op has started.
		This trap indicates that the file
		operation has started."
	 ::= { cerent15216EdfaEventsV2 5 }

cerent15216EdfaOpStarted  NOTIFICATION-TYPE
	OBJECTS		{ cerent15216EdfaOpSaveConfig,
			  cerent15216EdfaOpCutover,
			  cerent15216EdfaOpLoad,
			  cerent15216EdfaOpSrcFileName,
			  cerent15216EdfaOpTftpSrvAddr,
			  cerent15216EdfaOpReset,
			  cerent15216EdfaOpRequestId,
                          cerent15216EdfaOpDbFileName,
