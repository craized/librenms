	::= { cerent15216EdfaFfsFileEntry 2 }

cerent15216EdfaFfsFileStatus OBJECT-TYPE
	SYNTAX INTEGER {
			deleted(65528),
			ok(65532),
			renameSource(65534), 
			unInitialised(65535)
			}
