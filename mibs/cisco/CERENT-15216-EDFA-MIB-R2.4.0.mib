        DESCRIPTION
                "Database backup in progress."
        ::= { cerent15216EdfaAlarmsMisc 11 }

cerent15216EdfaAlarmDbRestore OBJECT-IDENTITY
        STATUS        current
        DESCRIPTION
                "Database restore in progress."
        ::= { cerent15216EdfaAlarmsMisc 12 }

cerent15216EdfaAlarmSwDownload OBJECT-IDENTITY
        STATUS        current
        DESCRIPTION
                "Software download in progress."
        ::= { cerent15216EdfaAlarmsMisc 13 }

