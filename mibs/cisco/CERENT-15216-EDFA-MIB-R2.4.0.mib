          TEXTUAL-CONVENTION,
          RowStatus,
	  DisplayString,
	  TimeStamp,
	  AutonomousType,
	  TruthValue,
	  DateAndTime,
          MacAddress
	               FROM SNMPv2-TC
          MODULE-IDENTITY,
          OBJECT-IDENTITY,
          OBJECT-TYPE,
          NOTIFICATION-TYPE,
          Integer32,
          Unsigned32,
          IpAddress
		       FROM SNMPv2-SMI
          cerentModules,
          cerentProducts
		       FROM CERENT-GLOBAL-REGISTRY;

cerent15216EdfaMibModule MODULE-IDENTITY
    LAST-UPDATED
        "0406100000Z" -- (2004/June/10)
    ORGANIZATION
        "Cisco Systems, Inc."
    CONTACT-INFO
        "
                support@cisco.com

        Postal: Cisco Systems, Inc.
                1435, North McDowell Blvd.
                Petaluma CA 94954 USA

           Tel: +1-877-323-7368
        "
    DESCRIPTION
        "
        This module provide the definitions for objects for
        managing ONS 15216 Erbium Doped Fiber Amplifier (EDFA).

	"

    ::= { cerentModules 60 }

-- TEXTUAL-CONVENTIONS --

Cerent15216EdfaBootFileName ::= TEXTUAL-CONVENTION
     STATUS	        current
     DESCRIPTION
			"The name of the file which may include a path
	                 component, e.g. dir1/dir2/name." 
    SYNTAX 	        OCTET STRING ( SIZE ( 0 .. 33  ) )

Cerent15216EdfaModuleIndex ::= TEXTUAL-CONVENTION
    STATUS	        current
    DESCRIPTION
		      	"Index of this module within the chassis."
    SYNTAX 	      	Integer32  ( 1 .. 63  )

Cerent15216EdfaHostName ::= TEXTUAL-CONVENTION
    STATUS	        current
    DESCRIPTION
		       	"A host name."
    SYNTAX 	       	OCTET STRING ( SIZE ( 0 .. 15  ) )

Cerent15216EdfaAlarmID ::= TEXTUAL-CONVENTION
	  STATUS	current
	  DESCRIPTION
			"Alarms defined for this product."
	  SYNTAX 	INTEGER  { unknownAlarm ( 1 ),
	                           lcrnt1 ( 2 ) ,
				   lcrnt2 ( 3 ) ,
				   ltmp1 ( 4 ) ,
				   ltmp2 ( 5 ) ,
				   lpout ( 6 ) ,
				   lpin ( 7 ) ,
				   gain ( 8 ) ,
				   ctmp ( 9 ) ,
				   powerBusA ( 10 ) ,
				   powerBusB ( 11 ) ,
				   dbBackup ( 12 ) ,
				   dbRestore ( 13 ) ,
				   swDownload (14 ) }

Cerent15216EdfaAlarmPriority ::= TEXTUAL-CONVENTION
    STATUS	        current
    DESCRIPTION
			"Priority of this alarm."
    SYNTAX 	        INTEGER  { noAlarm ( 1 ) ,
				   info ( 2 ) ,
				   minor ( 3 ) ,
				   major ( 4 ) ,
				   critical ( 5 ) }

Cerent15216EdfaBootImageName ::= TEXTUAL-CONVENTION
    STATUS	        current
    DESCRIPTION
			"The name of the loaded image."
    SYNTAX 	        OCTET STRING ( SIZE ( 0 .. 10  ) )

Cerent15216EdfaUnsigned8 ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
			"8 bit unsiged integer."
	SYNTAX 		Integer32  ( 0 .. 255  )

Cerent15216EdfaAlarmEventType ::= TEXTUAL-CONVENTION
   STATUS		current
   DESCRIPTION
	"The type of event.
	active(1)	active alarm
	cleared(2)	cleared alarm
	trap(3)		trap sent."
   SYNTAX 	INTEGER  { active  ( 1 ),
		           cleared ( 2 ),
			   trap    ( 3 ) }

Cerent15216EdfaIndex ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
			"Simple Index variable for table."
	SYNTAX 		Integer32  ( 1 .. 2048  )

Cerent15216EdfaDateAndTimeString ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
	    "A date and time.
	    The format is YYYY-MM-DD,hh:mm:ss:dd sTT:SS
            where
                  YYYY = year        (1996..)
                  MM   = month       (1..12)
                  DD   = day         (1..31)
                  hh   = hour        (0..23)
                  mm   = minute      (0..59)
                  ss   = second      (0..59)
                  dd   = deci-second (0..9)
              optionally followed by
                  s    = sign	     ('+' or '-')
                  TT   = timezone    (-23..23)
                  SS   = saving      (-59..59)"
	SYNTAX 	  OCTET STRING ( SIZE ( 0 .. 32  ) )

Cerent15216EdfaDescr32 ::= TEXTUAL-CONVENTION
	STATUS	        current
	DESCRIPTION
			"A short description of an entity."
	SYNTAX 		OCTET STRING ( SIZE ( 0 .. 32  ) )

Cerent15216EdfaDescr ::= TEXTUAL-CONVENTION
	STATUS	        current
	DESCRIPTION
			"A short description of an entity."
	SYNTAX 		OCTET STRING ( SIZE ( 0 .. 64  ) )

Cerent15216EdfaDescr10 ::= TEXTUAL-CONVENTION
	STATUS	        current
	DESCRIPTION
			"A short description of an entity."
	SYNTAX 		OCTET STRING ( SIZE ( 0 .. 10  ) )

Cerent15216EdfaUInteger8 ::= TEXTUAL-CONVENTION
	STATUS	        current
	DESCRIPTION
			"8 bit unsiged integer."
	SYNTAX 		Integer32 ( 0 .. 255  )

Cerent15216EdfaAlarmState ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
	               "State of this alarm."
	SYNTAX 	       INTEGER  { active   ( 1 ) ,
			          disabled ( 2 ) ,
				  cleared  ( 3 ) }

Cerent15216EdfaDCVoltage ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
	                "DC Voltage measurement is given in units of 0.1V.
                         For example if an object defined as this type has a
                         value of 52 then the value is actually 5.2V"
	SYNTAX INTEGER (0..9999)

Cerent15216EdfaPowerBusDCVoltage ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
	                "DC Voltage measurement is given in units of -0.1V.
                         For example if an object defined as this type 
                         has a value of 52 then the value is actually 5.2V."
	SYNTAX INTEGER (350..700)

Cerent15216EdfaPowerBusMode ::= TEXTUAL-CONVENTION
	STATUS 	       current
	DESCRIPTION
	              "Power BUS mode. Simplex mode requires only power to 
		      BUS A. Duplex Mode requires power to both BUS A & BUS B."
	SYNTAX 	      INTEGER { simplex( 1 ), 
	                        duplex( 2 ) }

Cerent15216EdfaPumpControlValue ::= TEXTUAL-CONVENTION
        STATUS        current
        DESCRIPTION
                "Laser pump value. In Units of 0.1mW, 0.1mA or 0.1dB depending 
                on pump control mode."
        SYNTAX INTEGER (0..3000)

Cerent15216EdfaImageType ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		       "What type of image is this."
	SYNTAX 		INTEGER  { bootLoader ( 1 ) ,
				   runTime ( 2 ) }

Cerent15216EdfaBootEntryIndex ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
			"The index of this boot entry for this
			module."
	SYNTAX 		Integer32  ( 1 .. 4  )

Cerent15216EdfaDateAndTimeFormsString ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"A date and time, in the forms format.
	        The format is MM/DD/YYYY hh:mm:ss
                where
                  MM   = month       (01..12)
                  DD   = day         (01..31)
                  YYYY = year        (1996..)
                  hh   = hour        (00..23)
                  mm   = minute      (00..59)
                  ss   = second      (00..59)"
	SYNTAX 		OCTET STRING ( SIZE ( 14 .. 19  ) )

Cerent15216EdfaSlot ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		      "The number of this slot within the
		      chassis.
	              -1 implies no slot."
	SYNTAX 		Integer32  ( -1 .. 15  )

Cerent15216EdfaActionStatus ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
			"The status of the action."
	SYNTAX 		INTEGER  { idle ( 1 ) ,
			           pending ( 2 ) ,
				   inProgress ( 3 ) ,
				   aborted ( 4 ) }

Cerent15216EdfaControl ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"To control a process or device."
	SYNTAX 		INTEGER  { enabled ( 1 ) ,
	                           disabled ( 2 ) }

Cerent15216EdfaPumpNumber ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Laser pump number."
	SYNTAX 		Integer32  ( 1 .. 2  )

Cerent15216EdfaRevision ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Revision Number Component."
	SYNTAX 		Integer32 ( 0 .. 255  )

Cerent15216EdfaAction ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
