-- boot table

cerent15216EdfaBootTable	OBJECT-TYPE
	SYNTAX		SEQUENCE  OF  Cerent15216EdfaBootEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION	"The boot source table.
			This table contains the boot sources used
	                to load the modules.  Each module tries
			it's sources in turn until an image is
			successfully loaded.
	                This table contains up to 4 entries per
			bootable module."
	::=  { cerent15216EdfaBootObjects  1 }

cerent15216EdfaBootEntry	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaBootEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION	"The boot entry."
	INDEX		{  cerent15216EdfaBootModuleIndex,
			   cerent15216EdfaBootEntryIndex  }
	::=  { cerent15216EdfaBootTable 1 }

Cerent15216EdfaBootEntry  ::=  SEQUENCE {
	cerent15216EdfaBootModuleIndex
			        Cerent15216EdfaModuleIndex,
	cerent15216EdfaBootEntryIndex
  			        Cerent15216EdfaBootEntryIndex,
	cerent15216EdfaBootType  
	                        Cerent15216EdfaBootSource,
	cerent15216EdfaBootIpAddress  
                                IpAddress,
	cerent15216EdfaBootFileName  
                                Cerent15216EdfaBootFileName
	}


cerent15216EdfaBootModuleIndex	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaModuleIndex
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The index of this module."
	::=  {  cerent15216EdfaBootEntry  1  }


cerent15216EdfaBootEntryIndex	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaBootEntryIndex
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The index of this boot entry for this
	                module."
	::=  {  cerent15216EdfaBootEntry  2  }


cerent15216EdfaBootType	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaBootSource
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The boot record type"
	DEFVAL			{ flash }
	::=  {  cerent15216EdfaBootEntry  3  }


cerent15216EdfaBootIpAddress	OBJECT-TYPE
	SYNTAX		IpAddress
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"IP address for boot server when using
	                TFTP or bootp, 0.0.0.0 otherwise."
	DEFVAL			{ '00000000'H }
	::=  {  cerent15216EdfaBootEntry  4  }


cerent15216EdfaBootFileName	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaBootFileName
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The file to be loaded.
	                 This may include a path component.  For
	                 example if BootType is TFTP, this object
	                 represents the location of the file relative
	                 to the TFTP server's root directory and may
	                 have a form like images/image1."
	::=  {  cerent15216EdfaBootEntry  5  }

-- boot image table

cerent15216EdfaBootImageTable	OBJECT-TYPE
	SYNTAX		SEQUENCE  OF
	                    Cerent15216EdfaBootImageEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION	"The Image software table.
	                This table contains the boot and running
	                software image name and revision
			information for each bootable module."
	::=  { cerent15216EdfaBootObjects  3 }

cerent15216EdfaBootImageEntry	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaBootImageEntry
	MAX-ACCESS	not-accessible
