-- edfa operations

cerent15216EdfaOpGroup	OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION	"Operations on the Edfa
		To perform an operation, the manager sets one of the following
                operations to perform, and provides values to the associated 
                fields:
		    Operation	                 Fields
		    cerent15216EdfaOpSaveConfig  cerent15216EdfaOpRequestId
		    cerent15216EdfaOpCutover	 cerent15216EdfaOpRequestId
		    cerent15216EdfaOpLoad	 cerent15216EdfaOpRequestId
					         cerent15216EdfaOpSrcFileName
					         cerent15216EdfaOpTftpSrvAddr
		    cerent15216EdfaOpReset	 cerent15216EdfaOpRequestId
		    cerent15216EdfaOpDbBackup	 cerent15216EdfaOpRequestId
					         cerent15216EdfaOpDbFileName
		    cerent15216EdfaOpDbRestore	 cerent15216EdfaOpRequestId
					         cerent15216EdfaOpDbFileName
                    
		A cerent15216EdfaOpStarted trap is generated whenever an 
                operation field is set to perform. Only a single operation 
                can be set to perform at any given time.

		To abort the operation the manager sets the 
		Operation field to abort.

		The cerent15216EdfaOpFinished trap will be generated at 
		the completion of the action or if the action is aborted.
                The cerent15216EdfaOpResult attribute indicates if the 
                last operation was successfull."
	::=  {  cerent15216EdfaActions  1  }

cerent15216EdfaOpStatus	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaActionStatus
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"The status of the current operation."
	DEFVAL		{ idle }
	::=  {  cerent15216EdfaOpGroup  1  }

cerent15216EdfaOpResult	OBJECT-TYPE
	SYNTAX		Integer32  ( -2147483647 .. 2147483647 )
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"This value is used to save the error
			result (if any) of the last operation.
			Success is indicated by a value of 0.

                        For DB backup and restore the following results
                        are possible:

                        0: Success.

                        1: Restore or backup operation cannot obtain the 
                           db semaphore. This error should never occur 
                           because the cerent15216EdfaOpGroup mib 
                           implementation does not allow more than one 
                           operation to be in progress at a time.

                        2: This will occur if there is a problem with 
                           opening cerent15216EdfaOpDbFileName. This could 
                           be because the file system is full, in the 
                           case of a restore the file does not exist, in 
                           the case of a backup the file already exists 
                           on the file system.

                        3: This will occur if the backup procedure cannot
                           obtain a config value.

                        4: This will occur during a restore if the crc in 
                           file specified by cerent15216EdfaOpDbFileName is 
                           not correct, ie corrupted backup file.

                        5: This will occur during a restore if there is a
                           parsing error. This could occur if the backup file
                           was created by a version of software that is not 
                           compatible with the current software version.

                        -1: Unknown internal failure." 
	DEFVAL		{ 0 }
	::=  {  cerent15216EdfaOpGroup  2  }

cerent15216EdfaOpRequestId	OBJECT-TYPE
	SYNTAX		Integer32  ( -2147483647 .. 2147483647 )
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The request identifier for this operation. 
	                This is provided to allow a mananger to match 
			results with actions."
	::=  {  cerent15216EdfaOpGroup  3  }

cerent15216EdfaOpSaveConfig	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaAction
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"To save the configuration of the Management &
	                Optical Modules, write 'perform' to this object." 
	DEFVAL		{ idle }
	::=  {  cerent15216EdfaOpGroup  4  }

cerent15216EdfaOpCutover	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaAction
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"When 'perform' is written to this
	                object, a cutover will occur where the 
	                cerent15216EdfaAlternateVersion software image will
                	be loaded after a Management Module reset. 

			The cutover operation is achieved automatically by
			swapping cerent15216EdfaBootEntry 1 & 2, then 
			performing a Management Module reset. 

			i.e. the pre-reset cerent15216EdfaAlternateVersion 
			becomes the post-reset cerent15216EdfaCurrentVersion
			and vice-versa."
	DEFVAL 		{ idle }
	::=  {  cerent15216EdfaOpGroup  5  }

cerent15216EdfaOpLoad	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaAction
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"When 'perform' is written to this
	                object, a new software image is downloaded via TFTP.

			Before setting this attribute, the source of the 
			new software image should be specified by setting
			cerent15216EdfaOpSrcFileName and 
			cerent15216EdfaOpTftpSrvAddr." 
	DEFVAL	      	{ idle }
	::=  {  cerent15216EdfaOpGroup  6  }

cerent15216EdfaOpSrcFileName	OBJECT-TYPE
	SYNTAX		DisplayString  ( SIZE ( 0 .. 127  ) )
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Source file name for the Load operation.
	                This must include the path of the file relative to 
			the TFTP server's root directory, eg. images/image1."
	::=  {  cerent15216EdfaOpGroup  7  }

cerent15216EdfaOpTftpSrvAddr	OBJECT-TYPE
	SYNTAX		IpAddress
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"TFTP file server address for the Load
			operation." 
	DEFVAL		{ '00000000'H }
	::=  {  cerent15216EdfaOpGroup  8  }

cerent15216EdfaOpReset	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaAction
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"This object will reset the Management Module and 
	                the Optical Module when the 'perform' value is 
			written to it. The reset will be delayed by
			a short period (approximately 10 seconds) to allow 
			SNMP response to be sent to the manager before the 
			reset is performed."
	DEFVAL		{ idle }
	::=  {  cerent15216EdfaOpGroup  9  }

cerent15216EdfaOpDbFileName	OBJECT-TYPE
	SYNTAX		DisplayString  ( SIZE ( 0 .. 20  ) )
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Db file name for the Db Backup and Db Restore 
                        Operations."
	::=  {  cerent15216EdfaOpGroup  10  }

cerent15216EdfaOpDbBackup OBJECT-TYPE
	SYNTAX		Cerent15216EdfaAction
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"When 'perform' is written to this
	                object, the amplifier identification data and 
                        configuration are written to the specified DB backup 
                        file.

			Before setting this attribute, the DB backup file
                        name must specified by setting 
                        cerent15216EdfaDbFileName." 
	DEFVAL	      	{ idle }
	::=  {  cerent15216EdfaOpGroup  11  }

cerent15216EdfaOpDbRestore	OBJECT-TYPE
	SYNTAX		Cerent15216EdfaAction
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"When 'perform' is written to this
	                object, the amplifier select configuration 
                        is restored from the specified DB backup 
                        file.

			Before setting this attribute, the DB backup file
                        name must specified by setting 
                        cerent15216EdfaDbFileName." 
	DEFVAL	      	{ idle }
	::=  {  cerent15216EdfaOpGroup  12  }


-- File System Directory table

cerent15216EdfaFfsFileTable  OBJECT-TYPE
	SYNTAX         SEQUENCE OF Cerent15216EdfaFfsFileEntry
	MAX-ACCESS     not-accessible
	STATUS         current
	DESCRIPTION
		"This table contains status information about
		the files within the file system."
	::= { cerent15216EdfaFfsDirectory 1 }

cerent15216EdfaFfsFileEntry    OBJECT-TYPE
	SYNTAX         Cerent15216EdfaFfsFileEntry
	MAX-ACCESS     not-accessible
	STATUS         current
	DESCRIPTION
	     "This file entry."
